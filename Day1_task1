#java script 
console.log("=== Task 1: Variables ===");
var name = "Celine";
let age = 18;
const city = "Coimbatore";

console.log("Name (var):", name);
console.log("Age (let):", age);
console.log("city (const):", city);

name = "C.W";
age = 19;

console.log("Modified Name:", name);
console.log("Modified Age:", age);
